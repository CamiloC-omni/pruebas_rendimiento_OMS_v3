{
    "setupTimeout": "10m",
    "summaryTimeUnit": "ms",
    "noConnectionReuse": true,
    "summaryTrendStats": ["avg", "min", "med", "max", "p(95)", "p(99)", "count"],
    "scenarios": {
        "shared_iterations_test": {
            "executor": "shared-iterations",
            "exec":"addQuantityTest",
            "iterations": 1,
            "vus": 1,
            "maxDuration": "30s"
        }
    }, "thresholds": {
    "http_reqs{group:::add_quantity}": ["count > 0"],
    "http_req_failed{group:::add_quantity}": ["rate<0.1"],
    "http_req_duration{group:::add_quantity}": ["p(95)<3000"],
    "http_req_waiting{group:::add_quantity}": ["p(95)<3000"]
  }
} 